{
  "name": "nodejs-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for consistent project structures and maintenance.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "fixtures": "echo \"No command specified\" && exit 0",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint .",
    "clean:coverage": "rm -Rf ./test/coverage",
    "pretest": "npm run clean:coverage",
    "test": "NODE_ENV=test nyc mocha --opts ./mocha.opts",
    "posttest": "npm run lint",
    "open:coverage": "open ./test/coverage/lcov-report/index.html",
    "projectsheet": "open $npm_package_studiohyperdrive_projectsheet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/district01/nodejs-boilerplate.git"
  },
  "author": "Thomas Bormans <thomas.bormans@studiohyperdrive.be>",
  "contributors": [
    "Thomas Bormans <thomas.bormans@studiohyperdrive.be>",
    "Fabian Meul <fabian.meul@studiohyperdrive.be>",
    "Niels Bril <niels.bril@studiohyperdrive.be>"
  ],
  "license": "ISC",
  "homepage": "https://bitbucket.org/district01/nodejs-boilerplate",
  "dependencies": {
    "@studiohyperdrive/api-docs": "1.0.0",
    "bcrypt": "3.0.2",
    "body-parser": "1.18.3",
    "connect-mongo": "2.0.3",
    "cookie-parser": "1.4.3",
    "eslint": "^5.11.1",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "glob": "7.1.1",
    "handlebars": "^4.0.12",
    "helmet": "3.14.0",
    "joi": "10.4.1",
    "mockery": "^2.1.0",
    "mongoose": "^5.4.1",
    "node-uuid": "^1.4.8",
    "nodemailer": "^5.0.0",
    "ramda": "^0.26.1",
    "session-file-store": "1.0.0",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "@studiohyperdrive/eslint-config": "3.0.2",
    "chai": "4.0.2",
    "chai-as-promised": "^7.1.1",
    "eslint-plugin-chai-friendly": "0.4.1",
    "mocha": "4.0.1",
    "mocha-multi": "1.0.1",
    "mongodb": "^3.1.12",
    "mongodb-memory-server": "^2.9.1",
    "nodemailer-mock": "^1.3.14",
    "nodemon": "1.18.9",
    "nyc": "^13.1.0",
    "pm2": "3.0.4",
    "proxyquire": "1.8.0",
    "supertest": "^3.0.0"
  },
  "studiohyperdrive": {
    "projectsheet": "https://bitbucket.org/district01/nodejs-boilerplate/overview"
  }
}
